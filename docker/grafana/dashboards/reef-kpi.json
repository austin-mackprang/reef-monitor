      "targets": [
        {
          "datasource": "InfluxDB_Apex",
          "query": "import \"strings\"\n\ntemp = from(bucket: \"apex\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"apex_probe\")\n  |> filter(fn: (r) => r[\"probe_type\"] == \"Temp\")\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\n  |> last()\n  |> map(fn: (r) => ({ \n    Parameter: \"Temperature\",\n    Current: string(v: r._value) + \" °F\",\n    \"Ideal Range\": \"76-80 °F\",\n    Status: if r._value >= 76.0 and r._value <= 80.0 then \"In Range\" else if r._value < 76.0 then \"Low\" else \"High\"\n  }))\n\npH = from(bucket: \"apex\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"apex_probe\")\n  |> filter(fn: (r) => r[\"probe_type\"] == \"pH\")\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\n  |> last()\n  |> map(fn: (r) => ({ \n    Parameter: \"pH\",\n    Current: string(v: r._value),\n    \"Ideal Range\": \"8.1-8.3\",\n    Status: if r._value >= 8.1 and r._value <= 8.3 then \"In Range\" else if r._value >= 7.8 and r._value < 8.1 then \"Caution\" else if r._value > 8.3 and r._value <= 8.4 then \"Caution\" else if r._value < 7.8 then \"Low\" else \"High\"\n  }))\n\norp = from(bucket: \"apex\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"apex_probe\")\n  |> filter(fn: (r) => r[\"probe_type\"] == \"ORP\")\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\n  |> last()\n  |> map(fn: (r) => ({ \n    Parameter: \"ORP\",\n    Current: string(v: r._value) + \" mV\",\n    \"Ideal Range\": \"250-400 mV\",\n    Status: if r._value >= 250.0 and r._value <= 400.0 then \"In Range\" else if r._value < 250.0 then \"Low\" else \"High\"\n  }))\n\nsal = from(bucket: \"apex\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"apex_probe\")\n  |> filter(fn: (r) => r[\"probe_type\"] == \"Cond\")\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\n  |> last()\n  |> map(fn: (r) => ({ \n    Parameter: \"Salinity\",\n    Current: string(v: r._value) + \" ppt\",\n    \"Ideal Range\": \"32-35 ppt\",\n    Status: if r._value >= 32.0 and r._value <= 35.0 then \"In Range\" else if r._value < 32.0 then \"Low\" else \"High\"\n  }))\n\nalk = from(bucket: \"apex\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"apex_probe\")\n  |> filter(fn: (r) => r[\"probe_type\"] == \"alk\")\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\n  |> last()\n  |> map(fn: (r) => ({ \n    Parameter: \"Alkalinity\",\n    Current: string(v: r._value) + \" dKH\",\n    \"Ideal Range\": \"8-12 dKH\",\n    Status: if r._value >= 8.0 and r._value <= 12.0 then \"In Range\" else if r._value < 8.0 then \"Low\" else \"High\"\n  }))\n\nca = from(bucket: \"apex\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"apex_probe\")\n  |> filter(fn: (r) => r[\"probe_type\"] == \"ca\")\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\n  |> last()\n  |> map(fn: (r) => ({ \n    Parameter: \"Calcium\",\n    Current: string(v: r._value) + \" ppm\",\n    \"Ideal Range\": \"380-450 ppm\",\n    Status: if r._value >= 380.0 and r._value <= 450.0 then \"In Range\" else if r._value < 380.0 then \"Low\" else \"High\"\n  }))\n\nmg = from(bucket: \"apex\")\n  |> range(start: -5m)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"apex_probe\")\n  |> filter(fn: (r) => r[\"probe_type\"] == \"mg\")\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\n  |> last()\n  |> map(fn: (r) => ({ \n    Parameter: \"Magnesium\",\n    Current: string(v: r._value) + \" ppm\",\n    \"Ideal Range\": \"1200-1400 ppm\",\n    Status: if r._value >= 1200.0 and r._value <= 1400.0 then \"In Range\" else if r._value < 1200.0 then \"Low\" else \"High\"\n  }))\n\nunion(tables: [temp, pH, orp, sal, alk, ca, mg])\n  |> sort(columns: [\"Parameter\"])",
          "refId": "A"
        }
